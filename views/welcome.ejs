<div class="mt-5 welcome-page">
	<div class="welcome-container container pt-5 pb-5">
		<img class="mb-5 mt-3" src="../sm.png" id="logo" />
		<a
			href="/users/login"
			class="btn-signup btn btn-light btn-block button login mt-2 mb-3 height padding"
			>Login</a
		>
		<a
			href="/users/register"
			class="btn-login btn btn-light btn-block button register mb-2 mt-4 height padding"
			>Register</a
		>
		<p id="forgot-password" class="mb-3 mt-3">Or Continue With</p>

		<a
			href="/auth/google"
			id="google-button"
			class="btn btn-block btn-google button mb-2 mt-2 padding"
		>
			<i class="fa fa-google"></i> Google
		</a>

		<a
			href="/auth/facebook"
			id="facebook-button"
			class="btn btn-block btn-facebook button facebook mb-3 mt-4 padding"
		>
			<i class="fa fa-facebook"></i> Facebook
		</a>
	</div>
</div>
